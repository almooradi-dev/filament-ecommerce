{
    "name": "almooradi/filament-ecommerce",
    "description": "Add e-Commerce features to filament app",
    "keywords": [
        "filament",
        "laravel",
        "filament-ecommerce",
        "ecommerce",
        "shop"
    ],
    "autoload": {
        "psr-4": {
            "Almooradi\\FilamentEcommerce\\": "src/",
            "Almooradi\\FilamentEcommerce\\Database\\": "database/"
        }
    },
    "authors": [
        {
            "name": "Al Mooradi",
            "email": "dev@almooradi.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "filament/filament": "^2.15",
        "spatie/laravel-package-tools": "^1.13.5",
        "illuminate/contracts": "^9.0 || ^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Almooradi\\FilamentEcommerce\\FilamentEcommerceServiceProvider"
            ]
        }
    }
}
